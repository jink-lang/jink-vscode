{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Jink",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.jink",
				"match": "\\b(if|else|elseif|import|export|void|return|delete|true|false|null|int|float|string|bool)\\b"
			}]
		},
		"strings": {
			"patterns": [
				{
					"include": "#string-double"
				},
				{
					"include": "#string-single"
				}
			]
		},
		"string-double": {
			"name": "string.quoted.double.jink",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"include": "#string-escape-character"
				}
			]
		},
		"string-single": {
			"name": "string.quoted.single.jink",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"include": "#string-escape-character"
				}
			]
		},
		"string-escape-character": {
			"name": "constant.character.escape.jink",
			"match": "\\\\."
		}
	},
	"scopeName": "text.jink"
}