{
	"Print to console": {
		"prefix": "print",
		"body": [
			"printf(\"$1\");$2"
		],
		"description": "Log output to console"
	},
  "Define a function": {
    "prefix": "fun",
    "body": [
      "fun $1($2)$3 {",
      "\t$4",
      "}"
    ],
    "description": "Define a function"
  },
  "Define a class": {
    "prefix": ["class", "cls"],
    "body": [
      "cls $1 = {",
      "\t$2",
      "}"
    ],
    "description": "Define a class"
  },
  "Define a type": {
    "prefix": ["type", "struct"],
    "body": [
      "type $1 = {",
      "\t$2",
      "}"
    ],
    "description": "Define a type (struct)"
  },
  "Define an enum": {
    "prefix": "enum",
    "body": [
      "enum $1 = {",
      "\t$2",
      "}"
    ],
    "description": "Define an enum"
  },
  "Extern declaration": {
    "prefix": "extern",
    "body": [
      "extern(\"$1\") $2($3);"
    ],
    "description": "Declare an external function"
  },
  "Import module": {
    "prefix": "import",
    "body": [
      "import $1;"
    ],
    "description": "Import a module"
  },
  "If statement": {
    "prefix": "if",
    "body": [
      "if ($1) {",
      "\t$2",
      "}"
    ],
    "description": "If statement"
  },
  "While loop": {
    "prefix": "while",
    "body": [
      "while ($1) {",
      "\t$2",
      "}"
    ],
    "description": "While loop"
  },
  "For loop": {
    "prefix": "for",
    "body": [
      "for ($1 in $2) {",
      "\t$3",
      "}"
    ],
    "description": "For loop"
  }
}